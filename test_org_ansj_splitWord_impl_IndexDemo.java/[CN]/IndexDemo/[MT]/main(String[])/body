{
  int all=0;
  String str="??????????????????????????????????????????,????????????!";
  long start=System.currentTimeMillis();
  for (int i=0; i < 1; i++) {
    all+=str.length();
    List<Term> paser=IndexAnalysis.paser(str);
    System.out.println(paser);
  }
  System.out.println("???????????????:" + (all * 1000.0 / (System.currentTimeMillis() - start)));
}
