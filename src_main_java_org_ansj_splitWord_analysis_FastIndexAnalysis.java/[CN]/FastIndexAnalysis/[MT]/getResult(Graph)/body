{
  List<Term> result=new LinkedList<Term>();
  int length=graph.terms.length - 1;
  Term term=null;
  for (int i=0; i < length; i++) {
    if ((term=graph.terms[i]) != null) {
      result.add(term);
      while ((term=term.next()) != null) {
        result.add(term);
      }
    }
  }
  return result;
}
