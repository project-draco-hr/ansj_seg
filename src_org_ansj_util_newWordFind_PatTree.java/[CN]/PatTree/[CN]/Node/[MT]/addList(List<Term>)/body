{
  Node node=null;
  Map<String,Node> tempNodes=this.nodes;
  Term term=null;
  for (int i=0; i < list.size(); i++) {
    term=list.get(i);
    node=tempNodes.get(term.getName());
    if (node != null) {
      node.count++;
    }
 else {
      node=new Node(term.getName());
      tempNodes.put(term.getName(),node);
    }
    tempNodes=node.nodes;
  }
}
