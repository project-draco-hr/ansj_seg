{
  BufferedReader br=null;
  try {
    cnMap=new HashMap<String,int[]>();
    br=MyStaticValue.getCompanReader();
    String temp=null;
    String[] strs=null;
    int[] cna=null;
    while ((temp=br.readLine()) != null) {
      strs=temp.split("\t");
      cna=new int[2];
      cna[0]=Integer.parseInt(strs[1]);
      cna[1]=Integer.parseInt(strs[2]);
      cnMap.put(strs[0],cna);
    }
  }
  finally {
    if (br != null)     br.close();
  }
}
