{
  Forest forest=getUserForestMap().get(name);
  if (forest != null) {
    return forest;
  }
  if (files == null || files.length == 0) {
    System.err.println("not find any library by name " + name);
    return null;
  }
  new DicManager(name).initLibraryByName(name);
  return getUserForestMap().get(name);
}
