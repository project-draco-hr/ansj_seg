{
  for (; i < charsLength; i++) {
    charHashCode=chars[i];
    end++;
switch (getStatement()) {
case 0:
      if (baseValue == chars[i]) {
        str=String.valueOf(chars[i]);
        offe=i;
        start=++i;
        end=0;
        baseValue=0;
        tempBaseValue=baseValue;
        return str;
      }
 else {
        i=start;
        start++;
        end=0;
        baseValue=0;
        break;
      }
case 2:
    i++;
  offe=start;
tempBaseValue=baseValue;
return DATDictionary.getItem(tempBaseValue).getName();
case 3:
offe=start;
start++;
i=start;
end=0;
tempBaseValue=baseValue;
baseValue=0;
return DATDictionary.getItem(tempBaseValue).getName();
}
}
if (start++ != i) {
i=start;
baseValue=0;
return allWords();
}
end=0;
baseValue=0;
i=0;
return null;
}
