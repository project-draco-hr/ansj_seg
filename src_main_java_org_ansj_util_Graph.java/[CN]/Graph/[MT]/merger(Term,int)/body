{
  Term term=null;
  if (terms[to] != null) {
    term=terms[to];
    while (term != null) {
      term.setPathScore(fromTerm);
      term=term.getNext();
    }
  }
 else {
    char c=chars[to];
    TermNatures tn=DATDictionary.getItem(c).termNatures;
    if (tn == null || tn == TermNatures.NULL) {
      tn=TermNatures.NULL;
    }
    terms[to]=new Term(String.valueOf(c),to,tn);
    terms[to].setPathScore(fromTerm);
  }
}
