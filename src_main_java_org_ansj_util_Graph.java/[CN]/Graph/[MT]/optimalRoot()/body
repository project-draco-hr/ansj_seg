{
  Term to=end;
  to.clearScore();
  Term from=null;
  while ((from=to.getFrom()) != null) {
    for (int i=from.getOffe() + 1; i < to.getOffe(); i++) {
      terms[i]=null;
    }
    if (from.getOffe() > -1) {
      terms[from.getOffe()]=from;
    }
    from.setNext(null);
    from.setTo(to);
    from.clearScore();
    to=from;
  }
  return root;
}
