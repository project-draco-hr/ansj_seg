{
  long start=System.currentTimeMillis();
  try {
    initArrays();
    MyStaticValue.LIBRARYLOG.info("init core library ok use time :" + (System.currentTimeMillis() - start));
  }
 catch (  Exception e) {
    e.printStackTrace();
    MyStaticValue.LIBRARYLOG.warning("init library core library error! ");
  }
}
