  Term term=null;
  while ((term=na.next()) != null) {
    allCount+=term.getName().length();
  }
  long end=System.currentTimeMillis();
  System.out.println(end - start);
  System.out.println("??? " + allCount + " ???????????????????????????:"+ (allCount * 1000.0 / (end - start)));
