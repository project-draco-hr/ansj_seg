{
  PatTree pt=new PatTree();
  List<Term> tempList=new ArrayList<Term>();
  for (  Term term : terms) {
    if (filter(term)) {
      tempList.add(term);
    }
 else {
      if (tempList.size() > 2) {
        pt.addList(tempList);
      }
      if (tempList.size() > 0) {
        if (tempList.size() < 10) {
          tempList.clear();
        }
 else {
          tempList=new ArrayList<Term>();
        }
      }
    }
  }
  return pt;
}
