{
  this.terms=terms;
  Term term=null;
  Branch branch=tire.getRoot();
  for (int i=0; i < terms.length; i++) {
    term=terms[i];
    if (term != null) {
      term.score=0;
      term.selfScore=0;
      if (term.getTermNatures().personAttr.flag) {
        termNext(term,branch,new Person(term));
      }
    }
  }
}
