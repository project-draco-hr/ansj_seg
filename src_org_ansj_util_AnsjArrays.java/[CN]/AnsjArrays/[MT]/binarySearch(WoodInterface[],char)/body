{
  int high=branches.length - 1;
  if (branches.length < 1) {
    return high;
  }
  int low=0;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    int cmp=branches[mid].compareTo(c);
    if (cmp < 0)     low=mid + 1;
 else     if (cmp > 0)     high=mid - 1;
 else     return mid;
  }
  return -1;
}
