{
  ToAnalysis.parse("test---aaaa??????????????????");
  BufferedReader reader=IOUtil.getReader("/Users/ansj/Documents/temp/test.txt",IOUtil.UTF8);
  String temp=null;
  List<String> all=new ArrayList<String>();
  int i=0;
  while ((temp=reader.readLine()) != null) {
    i++;
    if (i < 0) {
      continue;
    }
    all.add(temp);
    if (all.size() > 10000) {
      break;
    }
  }
  long start=System.currentTimeMillis();
  int allCount=0;
  for (int j=0; j < 1; j++) {
    for (    String string : all) {
      allCount+=string.length();
      ToAnalysis.parse(string);
    }
  }
  long end=System.currentTimeMillis();
  System.out.println(start - end);
  System.out.println("??? " + allCount + " ???????????????????????????:"+ (allCount * 1000.0 / (end - start)));
}
