{
  double[] sta=null;
  for (int i=0; i < status.length; i++) {
    sta=status[i];
    double sum=MatrixUtil.sum(sta);
    for (int j=0; j < sta.length; j++) {
      if (sta[j] == 0) {
        continue;
      }
      sta[j]=sta[j] / sum;
      sta[j]=-sta[j] * Math.log(sta[j]);
    }
  }
  double sum=MatrixUtil.sum(tagCount);
  for (int i=0; i < tagCount.length; i++) {
    tagPos[i]=tagCount[i] / sum;
  }
}
