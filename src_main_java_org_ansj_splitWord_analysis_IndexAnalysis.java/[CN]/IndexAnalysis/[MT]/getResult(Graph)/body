{
  Merger merger=new Merger(){
    @Override public List<Term> merger(){
      graph.walkPath();
      if (MyStaticValue.isNumRecognition)       NumRecognition.recognition(graph.terms);
      if (MyStaticValue.isNameRecognition)       new AsianPersonRecognition(graph.terms).recognition();
      new UserDefineRecognition(graph.terms,forests).recognition();
      return result();
    }
    /** 
 * ???????????????
 * @return
 */
    private List<Term> result(){
      List<Term> result=new LinkedList<Term>();
      Term term=null;
      String temp=null;
      int length=graph.terms.length - 1;
      for (int i=0; i < length; i++) {
        term=graph.terms[i];
        while (term != null) {
          result.add(term);
          temp=term.getName();
          term=term.getNext();
          if (term == null || term.getName().length() == 1 || temp.equals(term.getName())) {
            break;
          }
        }
      }
      setRealName(graph,result);
      return result;
    }
  }
;
  return merger.merger();
}
