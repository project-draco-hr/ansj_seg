{
  Term term=null;
  if (terms[to] != null) {
    term=terms[to];
    while (term != null) {
      term.setPathScore(fromTerm);
      term=term.getNext();
    }
  }
 else {
    char c=str.charAt(to);
    TermNatures tn=InitDictionary.termNatures[c];
    if (tn == null)     tn=TermNatures.NULL;
    terms[to]=new Term(String.valueOf(c),to,tn);
    terms[to].setPathScore(fromTerm);
  }
}
