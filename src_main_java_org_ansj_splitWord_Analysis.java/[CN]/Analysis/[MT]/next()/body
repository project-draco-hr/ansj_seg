{
  Term term=null;
  if (!terms.isEmpty()) {
    term=terms.poll();
    term.updateOffe(offe);
    return term;
  }
  String temp=br.readLine();
  offe=br.getStart();
  while (StringUtil.isBlank(temp)) {
    if (temp == null) {
      return null;
    }
 else {
      temp=br.readLine();
    }
  }
  analysisStr(temp);
  if (!terms.isEmpty()) {
    term=terms.poll();
    term.updateOffe(offe);
    return term;
  }
  return null;
}
