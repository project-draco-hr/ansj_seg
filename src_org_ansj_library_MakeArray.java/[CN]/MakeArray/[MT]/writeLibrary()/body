{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < base.length; i++) {
    if (base[i] > 0) {
      sb.append(i + "	" + words[i]+ "	"+ base[i]+ "	"+ check[i]+ "	"+ status[i]+ "	"+ natures[i]);
      sb.append("\n");
    }
  }
  IOUtil.Writer(InitDictionary.arraysPath,charEncoding,sb.toString());
}
