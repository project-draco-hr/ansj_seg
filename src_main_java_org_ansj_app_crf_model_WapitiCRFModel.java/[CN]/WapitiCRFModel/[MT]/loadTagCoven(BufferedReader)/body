{
  int[] conver=new int[Config.TAG_NUM];
  String temp=br.readLine();
  for (int i=0; i < Config.TAG_NUM; i++) {
    char c=br.readLine().split(":")[1].charAt(0);
switch (c) {
case 'S':
      conver[i]=Config.S;
    break;
case 'B':
  conver[i]=Config.B;
break;
case 'M':
conver[i]=Config.M;
break;
case 'E':
conver[i]=Config.E;
break;
default :
throw new Exception("err tag named " + c + " in model "+ temp);
}
}
return conver;
}
