{
  if (term.getName().trim().length() < 2) {
    return 0;
  }
  String pos=term.getNatrue().natureStr;
  Double posScore=POS_SCORE.get(pos);
  if (posScore == null) {
    posScore=1.0;
  }
 else   if (posScore == 0) {
    return 0;
  }
  if (titleLength > term.getOffe()) {
    return 5 * posScore;
  }
  return (length - term.getOffe()) * posScore / (double)length;
}
