{
  if (term.getName().matches("(?s)\\d.*")) {
    return 0;
  }
  if (term.getName().trim().length() < 2) {
    return 0;
  }
  String pos=term.getNatrue().natureStr;
  if (!pos.startsWith("n") || "num".equals(pos)) {
    return 0;
  }
  if (titleLength > term.getOffe()) {
    return 100;
  }
  return (length - term.getOffe()) / 6 * 100;
}
