{
  BufferedReader reader=IOUtil.getReader("/Users/ansj/Documents/??????/??????/library/name.dic","UTF-8");
  HashMap<String,int[]> hm=new HashMap<String,int[]>();
  String temp=null;
  String[] strs=null;
  int[] ints=null;
  int min=0;
  int C2=0, C3=0, C4=0, CN=0;
  while ((temp=reader.readLine()) != null) {
    if (StringUtil.isBlank(temp))     continue;
switch (temp.length()) {
case 2:
      C2++;
    break;
case 3:
  C3++;
break;
case 4:
C4++;
break;
default :
CN++;
break;
}
}
double sum=C2 + C3 + C4+ CN;
System.out.println(C2 / sum);
System.out.println(C3 / sum);
System.out.println(C4 / sum);
System.out.println(CN / sum);
}
