{
  if (minSplitWord != null) {
    return minSplitWord;
  }
  LOCK.lock();
  if (minSplitWord != null) {
    return minSplitWord;
  }
  try {
    LIBRARYLOG.info("begin init crf min model!");
    minSplitWord=new SplitWord(Model.loadModel(DicReader.getInputStream("crf/min.model")));
    LIBRARYLOG.info("load crf model min ok!");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    LOCK.unlock();
  }
  return minSplitWord;
}
