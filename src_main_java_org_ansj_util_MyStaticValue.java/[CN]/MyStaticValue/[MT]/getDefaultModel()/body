{
  if (model != null) {
    return model;
  }
  LOCK.lock();
  if (model != null) {
    return model;
  }
  try {
    LIBRARYLOG.info("begin init crf model!");
    model=Model.loadModel(DicReader.getInputStream("crf/template.ftl"),DicReader.getInputStream("crf/bayes.model"));
    LIBRARYLOG.info("load crf model ok!");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    LOCK.unlock();
  }
  return model;
}
